#include <iostream>
using namespace std;

int MaxNA(int *arr, int n)
{
	int i, sum = 0, max = 0;
	for (i = 0; i < n; i++)
	{
		sum = sum + arr[i];
		if (sum < 0)
		{
			sum = 0;
		}
		if (sum > max)
		{
			max = sum;
		}
	}
	return max;
}

int main()
{
	int i, n, arr[10];
	cin >> n;
	for (i = 0; i < n; i++)
	{
		cin >> arr[n];
	}
	cout << MaxNA(arr, n) << endl;
}